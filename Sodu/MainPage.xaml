<Page
    x:Class="Sodu.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Sodu"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:vm ="using:Sodu.ViewModel"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:Interactivity="using:Microsoft.Xaml.Interactivity"
    xmlns:Core="using:Microsoft.Xaml.Interactions.Core"
    xmlns:UC="using:Sodu.UC"
    x:Name="root"
    xmlns:page="using:Sodu.Pages"
    xmlns:Converter="using:Sodu.Converter"  
    DataContext="{Binding Instance.MainPageViewModelInstance,Source={StaticResource Locator}}"
    mc:Ignorable="d"
    RequestedTheme="{Binding Instance.SettingPageViewModelInstance.Theme,Source={StaticResource Locator} }">

    <Page.Resources>
        <Converter:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"></Converter:BooleanToVisibilityConverter>
        <Converter:RevertBooleanToVisibilityConverter x:Key="RevertBooleanToVisibilityConverter"></Converter:RevertBooleanToVisibilityConverter>
    </Page.Resources>

    <Grid x:Name="HeaderGrid"  Background="{ThemeResource PageBackground}">
        <SplitView x:Name="Splitter"  Background="Transparent"   IsPaneOpen="{Binding IsLeftPanelOpen,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" OpenPaneLength="200" DisplayMode="Overlay" CompactPaneLength="70">
            <SplitView.Pane >
                <RelativePanel Margin="0,0,0,0"  >
                    <ScrollViewer VerticalAlignment="Stretch"  Background="{ThemeResource LeftPanelBackground}"   Margin="0,0,0,0" RelativePanel.AlignTopWithPanel="True"  RelativePanel.AlignBottomWithPanel="True"  HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto">
                        <Grid  Background="Transparent" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="auto"/>
                            </Grid.RowDefinitions>

                            <ToggleButton Style="{StaticResource CustomToggleButtonStyle}" Grid.Column="0"   Command="{Binding Instance.MainPageViewModelInstance.IsLeftPanelOpenCommand,Source={StaticResource Locator}}" VerticalAlignment="Stretch" Background="Transparent">
                                <ToggleButton.Content>
                                    <FontIcon x:Name="Hamburger" FontFamily="Segoe MDL2 Assets" Glyph="&#xE700;"   Margin="0,0,0,0"/>
                                </ToggleButton.Content>
                            </ToggleButton>

                            <StackPanel BorderThickness="0"  RelativePanel.AlignTopWithPanel="True" Margin="0,0,0,0"  Orientation="Vertical" Grid.Row="1" >
                                <AppBarButton  Icon="Home"   Label="首页" 
                                           Width="{Binding ElementName=Splitter,Path= OpenPaneLength,UpdateSourceTrigger=PropertyChanged}"  
                                           IsCompact="False"  HorizontalAlignment="Left"  Command="{Binding HomePageCommand}" Style="{StaticResource CustomAppBarButtonStyle}" 
                                               />

                                <AppBarButton  Icon="Library"   Label="本地图书" 
                                           Width="{Binding ElementName=Splitter,Path= OpenPaneLength,UpdateSourceTrigger=PropertyChanged}"  
                                           IsCompact="False"  HorizontalAlignment="Left"  Command="{Binding LocalBooksCommand}" Style="{StaticResource CustomAppBarButtonStyle}" 
                                                >
                                </AppBarButton>

                                <AppBarButton  Icon="Download"   Label="下载中心" 
                                           Width="{Binding ElementName=Splitter,Path= OpenPaneLength,UpdateSourceTrigger=PropertyChanged}"  
                                           IsCompact="False"  HorizontalAlignment="Left"  Command="{Binding DownLoadCenterCommand}" Style="{StaticResource CustomAppBarButtonStyle}"
                                                >

                                </AppBarButton>

                                <AppBarButton  Icon="Clock"   Label="历史记录" 
                                           Width="{Binding ElementName=Splitter,Path= OpenPaneLength,UpdateSourceTrigger=PropertyChanged}"  
                                           IsCompact="False"  HorizontalAlignment="Left"  Command="{Binding ReadHistoryCommand}" Style="{StaticResource CustomAppBarButtonStyle}"
                                                >
                                </AppBarButton>

                                <AppBarButton  Icon="Like"   Label="关于"  Command="{Binding HelpCommand}"
                                           Width="{Binding ElementName=Splitter,Path= OpenPaneLength,UpdateSourceTrigger=PropertyChanged}"  
                                           IsCompact="False"  HorizontalAlignment="Left"   Style="{StaticResource CustomAppBarButtonStyle}"
                                                >

                                </AppBarButton>
                            </StackPanel>

